/users
  /{userId}
    - name
    - email
    - address
    - phoneNumber
    - role

/sellers
  /{sellerId}
    - storeName
    - storeDescription

/products
  /{productId}
    - name
    - description
    - price
    - stock
    - categoryId
    - sellerId

/categories
  /{categoryId}
    - name
    - description

/orders
  /{orderId}
    - userId
    - totalAmount
    - status

/orderDetails
  /{orderDetailId}
    - orderId
    - productId
    - quantity
    - price

/payments
  /{paymentId}
    - orderId
    - paymentMethod
    - amount
    - paymentDate

